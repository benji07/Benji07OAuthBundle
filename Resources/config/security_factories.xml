<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="benji07.oauth.security.factory.class">Benji07\Bundle\OAuthBundle\DependencyInjection\Security\Factory\OAuthFactory</parameter>
    </parameters>

    <services>
        <service id="benji07.oauth.security.factory" class="%benji07.oauth.security.factory.class%" public="false">
            <tag name="security.listener.factory" />
        </service>
    </services>
</container>